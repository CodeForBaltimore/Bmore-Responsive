{
  "name": "bmore-responsive",
  "version": "1.0.0",
  "description": "An emergency response API",
  "main": "/src/app.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "test": "nyc mocha --require @babel/register --require @babel/polyfill \"src/tests/**/*.spec.js\" --exit",
    "lint": "xo --ignore **/*.spec.js",
    "format": "xo --fix --ignore **/*.spec.js",
    "db-create": "sequelize db:migrate",
    "db-delete": "sequelize db:migrate:undo:all",
    "db-seed": "sequelize db:seed:all",
    "db-unseed": "sequelize db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeForBaltimore/Bmore-Responsive.git"
  },
  "keywords": [
    "emergency",
    "response"
  ],
  "author": "Code for Baltimore",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/CodeForBaltimore/Bmore-Responsive/issues"
  },
  "homepage": "https://github.com/CodeForBaltimore/Bmore-Responsive#readme",
  "dependencies": {
    "@babel/core": "7.8.4",
    "@babel/node": "7.8.4",
    "aws-sdk": "2.641.0",
    "chai": "4.2.0",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-request-id": "1.4.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.15",
    "mocha": "7.1.1",
    "morgan": "1.9.1",
    "nodemon": "2.0.2",
    "pg": "7.18.2",
    "random-words": "1.1.0",
    "sequelize": "5.21.4",
    "serverless": "^1.66.0",
    "supertest": "4.0.2",
    "swagger-ui-express": "4.1.3",
    "uuid4": "1.1.4",
    "validator": "12.2.0"
  },
  "devDependencies": {
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/register": "7.8.3",
    "babel-loader": "8.0.6",
    "codecov": "3.6.5",
    "nyc": "15.0.0",
    "sequelize-cli": "5.5.1",
    "serverless-domain-manager": "^3.3.1",
    "serverless-dotenv-plugin": "^2.3.2",
    "serverless-offline": "^5.12.1",
    "serverless-prune-plugin": "^1.4.2",
    "serverless-stage-manager": "^1.0.5",
    "serverless-webpack": "5.3.1",
    "webpack": "^4.2.0",
    "webpack-cli": "3.3.11",
    "webpack-node-externals": "1.7.2",
    "xo": "0.26.0"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
