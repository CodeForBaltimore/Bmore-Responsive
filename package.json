{
  "name": "bmore-responsive",
  "version": "1.3.3",
  "description": "An API-driven CRM (Civic Relationship Management) system.",
  "main": "src/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "test": "nyc mocha --require @babel/register --require @babel/polyfill \"src/tests/**/*.spec.js\" --exit",
    "lint": "xo --ignore **/*.spec.js",
    "format": "xo --fix --ignore **/*.spec.js",
    "db-create": "sequelize db:migrate",
    "db-delete": "sequelize db:migrate:undo:all",
    "db-seed": "sequelize db:seed:all",
    "db-unseed": "sequelize db:seed:undo:all",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeForBaltimore/Bmore-Responsive.git"
  },
  "keywords": [
    "emergency",
    "response"
  ],
  "author": "Code for Baltimore",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/CodeForBaltimore/Bmore-Responsive/issues"
  },
  "homepage": "https://github.com/CodeForBaltimore/Bmore-Responsive#readme",
  "dependencies": {
    "@babel/core": "7.10.5",
    "@babel/node": "7.10.5",
    "casbin": "4.7.2",
    "casbin-sequelize-adapter": "2.1.0",
    "chai": "4.2.0",
    "complexity": "0.0.6",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-rate-limit": "5.1.3",
    "express-request-id": "1.4.1",
    "helmet": "3.23.3",
    "json2csv": "^5.0.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.19",
    "mocha": "7.2.0",
    "morgan": "1.10.0",
    "nodemailer": "^6.4.8",
    "nodemon": "2.0.4",
    "nunjucks": "^3.2.1",
    "pg": "7.18.2",
    "random-words": "1.1.1",
    "sequelize": "5.22.3",
    "snyk": "^1.362.0",
    "supertest": "4.0.2",
    "swagger-ui-express": "4.1.4",
    "tls": "0.0.1",
    "uuid4": "1.1.4",
    "validator": "12.2.0"
  },
  "devDependencies": {
    "@babel/compat-data": "7.8.6",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/register": "7.8.3",
    "all-contributors-cli": "6.14.1",
    "chakram": "^1.5.0",
    "codecov": "3.7.2",
    "nyc": "15.0.0",
    "sequelize-cli": "5.5.1",
    "xo": "0.26.0"
  },
  "resolutions": {
    "@babel/preset-env": "7.8.4"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "snyk": true
}
